# Example environment variables for Azure OpenAI usage

# Provide your Azure OpenAI endpoint URL
AZURE_OPENAI_ENDPOINT=

# Provide your Azure OpenAI API key
AZURE_OPENAI_API_KEY=

# Provide your Chat Model Deployment name (e.g., gpt-o-1)
AZURE_OPENAI_DEPLOYMENT_NAME=

# Provide your Azure OpenAI API version (e.g., 2023-06-01-preview)
AZURE_OPENAI_API_VERSION=

# (Optional) Provide the embedding deployment name if needed
AZURE_OPENAI_EMBEDDING_DEPLOYMENT_NAME=